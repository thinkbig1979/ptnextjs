{
  "collectionName": "components_shared_social_medias",
  "info": {
    "displayName": "Social Media Links",
    "description": "Social media profile and page links",
    "icon": "share"
  },
  "options": {},
  "attributes": {
    "facebook": {
      "type": "string",
      "maxLength": 255,
      "required": false,
      "regex": "^https://www\\.facebook\\.com/.*",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "twitter": {
      "type": "string",
      "maxLength": 255,
      "required": false,
      "regex": "^https://(www\\.)?(twitter\\.com|x\\.com)/.*",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "linkedin": {
      "type": "string",
      "maxLength": 255,
      "required": false,
      "regex": "^https://([a-z]+\\.)?linkedin\\.com/(company|in)/.*",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "instagram": {
      "type": "string",
      "maxLength": 255,
      "required": false,
      "regex": "^https://www\\.instagram\\.com/.*",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "youtube": {
      "type": "string",
      "maxLength": 255,
      "required": false,
      "regex": "^https://www\\.youtube\\.com/(c/|channel/|user/).*",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}