{
  "collectionName": "components_shared_seos",
  "info": {
    "displayName": "SEO Metadata",
    "description": "Search engine optimization metadata",
    "icon": "search"
  },
  "options": {},
  "attributes": {
    "meta_title": {
      "type": "string",
      "maxLength": 60,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "meta_description": {
      "type": "text",
      "maxLength": 160,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "keywords": {
      "type": "text",
      "maxLength": 255,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "og_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "canonical_url": {
      "type": "string",
      "maxLength": 255,
      "required": false,
      "regex": "^https?://[^\\s]+$",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "no_index": {
      "type": "boolean",
      "default": false,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}